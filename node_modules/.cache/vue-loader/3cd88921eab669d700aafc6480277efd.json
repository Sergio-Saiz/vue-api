     }\n      })\n      .then((res) => {\n        console.log(res.data)\n          this.plant.name = res.data.name,\n          this.plant.description = res.data.description,\n          this.plant.germination = res.data.germination,\n          this.plant.xdistance = res.data.xdistance,\n          this.plant.ydistance = res.data.ydistance,\n          this.plant.siembra_directa = res.data.siembra_directa,\n          this.plant.semillero = res.data.semillero,\n          this.plant.trasplante = res.data.trasplante,\n          this.plant.cosecha = res.data.cosecha,\n          this.plant.owner = this.$uuid.v4(),\n      })\n      .catch((error) => {\n        console.error(error)\n      })\n    },\n\n\n  }\n}\n</script>"]}]}