{"ast":null,"code":"import { mapState, mapActions } from 'vuex';\nimport axios from 'axios';\nimport { uuid } from 'vue-uuid';\nexport default {\n  computed: {\n    listPlant(formPlant) {\n      // `this` points to the component instance\n      return this.plants;\n    },\n\n    ...mapState(['token', 'user', 'userId'])\n  },\n\n  mounted() {\n    this.searchPlant(this.formPlant);\n  },\n\n  data() {\n    return {\n      formPlant: {\n        name: null,\n        description: null,\n        germination: null,\n        xdistance: null,\n        ydistance: null,\n        siembra_directa: null,\n        semillero: null,\n        trasplante: null,\n        cosecha: null\n      },\n      plants: {}\n    };\n  },\n\n  methods: {\n    searchPlant(formPlant) {\n      axios({\n        method: 'get',\n        url: 'http://localhost:250/api/v1/plants',\n        headers: {\n          'Authorization': `Bearer ${this.token}`\n        },\n        params: {\n          name: this.formPlant.name,\n          description: this.formPlant.description,\n          germination: this.formPlant.germination,\n          xdistance: this.formPlant.xdistance,\n          ydistance: this.formPlant.ydistance,\n          siembra_directa: this.formPlant.siembra_directa,\n          semillero: this.formPlant.semillero,\n          trasplante: this.formPlant.trasplante,\n          cosecha: this.formPlant.cosecha\n        }\n      }).then(res => {\n        console.log(res.data['hydra:member']);\n        this.plants = res.data['hydra:member'];\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8BA;AACA;AACA;AACA;EACAA;IAEAC;MACA;MACA;IACA,CALA;;IAOA;EAPA,CADA;;EAUAC;IACA;EACA,CAZA;;EAaAC;IACA;MACAC;QACAC,UADA;QAEAC,iBAFA;QAGAC,iBAHA;QAIAC,eAJA;QAKAC,eALA;QAMAC,qBANA;QAOAC,eAPA;QAQAC,gBARA;QASAC;MATA,CADA;MAYAC;IAZA;EAcA,CA5BA;;EA6BAC;IACAC;MACAC;QACAC,aADA;QAEAC,yCAFA;QAGAC;UACA;QADA,CAHA;QAMAC;UACAhB,yBADA;UAEAC,uCAFA;UAGAC,uCAHA;UAIAC,mCAJA;UAKAC,mCALA;UAMAC,+CANA;UAOAC,mCAPA;UAQAC,qCARA;UASAC;QATA;MANA,GAkBAS,IAlBA,CAkBAC;QACAC;QACA;MACA,CArBA,EAsBAC,KAtBA,CAsBAC;QACAF;MACA,CAxBA;IAyBA;;EA3BA;AA7BA","names":["computed","listPlant","mounted","data","formPlant","name","description","germination","xdistance","ydistance","siembra_directa","semillero","trasplante","cosecha","plants","methods","searchPlant","axios","method","url","headers","params","then","res","console","catch","error"],"sourceRoot":"src/views","sources":["Search.vue"],"sourcesContent":["<template>\n  <div class=\"dashboard\">\n    <h1>Semillas</h1>\n    <p>Usuario: {{userId}}</p>\n        <form @submit.prevent=\"searchPlant(formPlant)\">\n            name: <input style=\"border: 0\" type=\"text\" placeholder=\"name\" v-model=\"formPlant.name\">\n            description: <input style=\"border: 0\" type=\"text\" placeholder=\"description\" v-model=\"formPlant.description\">\n            germination: <input style=\"border: 0\" type=\"text\" placeholder=\"germination\" v-model=\"formPlant.germination\">\n            xdistance: <input style=\"border: 0\" type=\"text\" placeholder=\"xdistance\" v-model=\"formPlant.xdistance\">\n            ydistance: <input style=\"border: 0\" type=\"text\" placeholder=\"ydistance\" v-model=\"formPlant.ydistance\">\n            siembra_directa: <input style=\"border: 0\" type=\"text\" placeholder=\"siembra_directa\" v-model=\"formPlant.siembra_directa\">\n            semillero: <input style=\"border: 0\" type=\"text\" placeholder=\"semillero\" v-model=\"formPlant.semillero\">\n            trasPlante: <input style=\"border: 0\" type=\"text\" placeholder=\"trasPlante\" v-model=\"formPlant.trasformPlante\">\n            cosecha: <input style=\"border: 0\" type=\"text\" placeholder=\"cosecha\" v-model=\"formPlant.cosecha\">\n            <button type=\"submit\">Buscar</button>\n        </form>\n\n        <div class=\"row mx-3\">\n            <div v-for=\"plant in plants\" class=\"card col-2 mx-2 my-2\" style=\"width: 18rem;\">\n                <img class=\"card-img-top\" src=\"\" alt=\"Card image cap\">\n                <div class=\"card-body\">\n                    <h5 class=\"card-title\">{{plant.name}}</h5>\n                    <p class=\"card-text\">COsecha: {{plant.cosecha}}</p>\n                    <a href=\"#\" class=\"btn btn-primary\">Go somewhere</a>\n                </div>\n                </div>\n            </div>\n        </div>\n</template>\n<script>\nimport {mapState, mapActions} from 'vuex'\nimport axios from 'axios';\nimport { uuid } from 'vue-uuid'; \nexport default {\n  computed: {\n    \n    listPlant(formPlant) {\n      // `this` points to the component instance\n      return this.plants\n    },\n\n    ...mapState(['token', 'user', 'userId'])\n  },\n  mounted(){\n    this.searchPlant(this.formPlant)\n  },\n  data() {\n    return {\n        formPlant: {\n          name: null,\n          description: null,\n          germination: null,\n          xdistance: null,\n          ydistance: null,\n          siembra_directa: null,\n          semillero: null,\n          trasplante: null,\n          cosecha: null,\n        },\n        plants: {},\n    }\n  },\n  methods: {\n    searchPlant(formPlant) {\n      axios({\n        method: 'get',\n        url: 'http://localhost:250/api/v1/plants',\n        headers: {\n                'Authorization': `Bearer ${this.token}`\n                },\n        params: {\n          name: this.formPlant.name,\n          description: this.formPlant.description,\n          germination: this.formPlant.germination,\n          xdistance: this.formPlant.xdistance,\n          ydistance: this.formPlant.ydistance,\n          siembra_directa: this.formPlant.siembra_directa,\n          semillero: this.formPlant.semillero,\n          trasplante: this.formPlant.trasplante,\n          cosecha: this.formPlant.cosecha,\n        }\n      })\n      .then((res) => {\n        console.log(res.data['hydra:member'])\n        this.plants = res.data['hydra:member']\n      })\n      .catch((error) => {\n        console.error(error)\n      })\n    },\n\n\n  }\n}\n</script>"]},"metadata":{},"sourceType":"module"}